{
  "name": "front-toolkit",
  "version": "0.0.1",
  "description": "A react library developed with dumi",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "pnpm --filter \"...[origin/main]\" build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "test": "jest",
    "test-c": "jest --coverage",
    "test:jsx-style-px-to-rem": "jest ./packages/jsx-style-px-to-tem-babel-plugin/tests",
    "test-c:jsx-style-px-to-rem": "jest ./packages/jsx-style-px-to-tem-babel-plugin/tests",
    "docs:preview": "dumi preview",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "change:version": "pnpm changeset && pnpm changeset version && pnpm build",
    "release": "pnpm login:repository && changeset publish && git push origin --tags",
    "login:repository": "sh scripts/pnpm-login.sh",
    "create": "node ./scripts/create-template.js"
  },
  "authors": [
    "997763513@qq.com"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "packages/**/*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "packages/**/*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "packages/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "packages/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "dumi": "^2.4.13",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1",
    "ts-jest": "^29.2.5"
  }
}
